\AtBeginDocument {
    \raggedbottom
    \raggedcolumns
    \dim_set:Nn \headheight { 32pt }
    \dim_set:Nn \footskip   { 32pt }
    \dim_set:Nn \topskip    { 0mm  }
    \dim_set:Nn \parskip    { 6pt  }
    \dim_set:Nn \columnsep  { 32pt }
    \RenewExpandableDocumentCommand \arraystretch { } { 1.2 }
}
