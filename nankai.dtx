\NewDocumentCommand \PrintBibliography { } {
    \dim_set:Nn \bibitemsep { 3pt }
    \AtNextBibliography { \small }
    \bool_if:NT \c_@@_breaks_bool { \clearpage }
    \group_begin:
    \raggedright
    \setstretch { 0.95 }
    \printbibliography
}
