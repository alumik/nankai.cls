% \begin{macro}{\@@_page:}
%
% Display the page number and the total number of pages.
%
%    \begin{macrocode}
\cs_new:Nn \@@_page:
    {
        \bool_if:NTF \c_@@_cn_bool
            { 第~\thepage{}~页（共~\pageref*{LastPage}~页） }
            { Page~\#\thepage{}~of~\pageref*{LastPage}      }
    }
%    \end{macrocode}
% \end{macro}
%
% \changes{v2.1}{2025/05/16}{Do not use small caps for Chinese text.}
% \changes{v2.1}{2025/05/16}{Change ``It'' in the draft watermark to ``This''.}
%
% Then, we configure headers and footers using \pkg{fancyhdr}.
%
%    \begin{macrocode}
\RequirePackage { fancyhdr }
\pagestyle { fancy }
\RenewExpandableDocumentCommand \headrulewidth { } { 0pt }
\fancyhf { }
%    \end{macrocode}
%
% Add bar and draft watermarks.
%
%    \begin{macrocode}
\fancyfoot [ L ] {
    \@@_bar:
    \bool_if:NT \c_@@_authordraft_bool
        {
            \begin { textblock } { 14 } [ 0.5, 0.5 ] ( 8, 8 )
                \tikz
                \node [ minimum~width = 14 \TPHorizModule ] {
                    \fontsize { 64 } { 64 }
                    \selectfont
                    \sffamily
                    \color { gray!20 }
                    \rotatebox { 30 } {
                        \bool_if:NTF \c_@@_cn_bool
                            { 草\quad 稿               }
                            { \scshape This~is~a~draft }
                    }
                };
            \end { textblock }
        }
}
%    \end{macrocode}
%
% Add Nankai logo.
%
%    \begin{macrocode}
\fancyhead [ L ] {
    \int_compare:nT { \c@page != 1 }
        {
            \bool_if:nF
                { \c_@@_nonnankai_bool || \c_@@_boring_bool }
                {
                    \begin { textblock } { 8 } [ 0, 0 ] ( 1.2, 0.2 )
                        \@@_logo:n {
                            \bool_if:NTF \c_@@_slides_bool { 1.8 } { 1 }
                        }
                    \end { textblock }
                }
        }
}
%    \end{macrocode}
%
% Add document information.
%
%    \begin{macrocode}
\fancyfoot [ R ] {
    \bool_if:NTF \c_@@_boring_bool
        {
            \bool_if:NF \c_@@_nonnankai_bool
                {
                    \bool_if:NTF \c_@@_slides_bool
                        {
                            \begin { textblock }
                                { 3 }
                                [ 1, 0.5 ]
                                ( 15.5, 15.25 )
                                \raggedleft
                                \@@_logo:n { 1.4 }
                            \end { textblock }
                        }
                        {
                            \begin { textblock }
                                { 3 }
                                [ 1, 0.5 ]
                                ( 15, 15.25 )
                                \raggedleft
                                \@@_logo:n { 1 }
                            \end { textblock }
                        }
                }
            \bool_if:NF \c_@@_noinstitute_bool
                {
                    \begin { textblock }{ 8 } [ 0, 0.5 ] ( 1, 15.25 )
                        \@@_info:n {
                            \raggedright
                            \tl_use:N \g_@@_institute_tl
                        }
                    \end { textblock }
                }
            \bool_if:nF
                {
                    \c_@@_nopaging_bool ||
                    \int_compare_p:n { \c@page = 1 }
                }
                {
                    \bool_if:NTF \c_@@_nonnankai_bool
                        {
                            \begin { textblock } { 7 } [ 1, 0.5 ]
                                ( 15, 15.25 )
                        }
                        {
                            \begin { textblock } { 4 } [ 1, 0.5 ]
                                ( 12, 15.25 )
                        }
                        \@@_info:n { \raggedleft \@@_page: }
                    \end { textblock }
                }
        }
        {
            \begin { textblock } { 8 } [ 0, 1 ] ( 1.2, 15.8 )
                \int_compare:nT { \c@page != 1 }
                    {
                        \@@_info:n {
                            \raggedright
                            \bool_if:NF \c_@@_anonymous_bool
                                {
                                    \@author
                                    \bool_if:NF \c_@@_noinstitute_bool
                                        {
                                            \leavevmode
                                            \\
                                        }
                                }
                            \bool_if:NF \c_@@_noinstitute_bool
                                { \tl_use:N \g_@@_institute_tl }
                        }
                    }
            \end { textblock }
            \begin { textblock } { 8 } [ 1, 1 ] ( 15.8, 15.8 )
                \@@_info:n {
                    \raggedleft
                    \int_compare:nT { \c@page != 1 }
                        {
                            \bool_if:NF \c_@@_nopaging_bool
                            {
                                \@@_page:
                                \bool_if:NF \c_@@_nodate_bool
                                    {
                                        \\
                                    }
                              }
                        }
                    \bool_if:NF \c_@@_nodate_bool { \DTMnow }
                }
            \end { textblock }
        }
}
%    \end{macrocode}
