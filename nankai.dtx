% \begin{environment}{abstract}
%    \begin{macrocode}
\RenewDocumentEnvironment { abstract } { }
    {
        \center
        \minipage { 0.9 \linewidth }
        \begin { center }
            \bool_if:NTF \c_@@_cn_bool
                { \bfseries }
                { \scshape  }
            \abstractname
        \end{ center }
        \small
        \bool_if:NT \c_@@_cn_bool
            { \dim_set:Nn \parindent { 2 \ccwd } }
        \indent\indent
        \ignorespaces
    }
    {
        \endminipage
        \endcenter
    }
%    \end{macrocode}
% \end{environment}
