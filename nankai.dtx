% \begin{macro}{\maketitle}
%    \begin{macrocode}
\RenewDocumentCommand \maketitle { } {
    \group_begin:
    \dim_set:Nn \parindent { 0pt }
    \vspace* { 18pt }
    {
        \begin { spacing } { 0.9 }
            \Huge
            \raggedright
            \bfseries
            \bool_if:NT \c_@@_boring_bool { \color { NankaiPurple } }
            \@title
            \par
        \end { spacing }
    }
    \tl_if_empty:eF { \g_@@_subtitle_tl }
        {
            {
                \color{gray!50!black}
                \large
                \raggedright
                \tl_use:N \g_@@_subtitle_tl
                \par
            }
        }
    \vspace { 2em }
    \bool_if:NF \c_@@_noinstitute_bool
        {
            \tl_if_empty:eF { \g_@@_institute_tl }
                {
                    \tl_use:N \g_@@_institute_tl
                    \par
                }
        }
    \bool_if:NTF \c_@@_anonymous_bool
        {
            \bool_if:NTF \c_@@_cn_bool
                { 匿名作者 }
                { \textsc { Anonymous~Author(s) } }
        }
        { \textsc { \@author } }
    \par
    \vspace { 2em }
    \group_end:
}
%    \end{macrocode}
% \end{macro}
