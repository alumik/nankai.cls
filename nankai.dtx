% \begin{environment}{note}
%    \begin{macrocode}
\NewDocumentEnvironment { note } { }
    {
        \bool_if:NTF \c_@@_boring_bool
            { \tl_const:Nn \c_@@_notecolor_tl { gray } }
            { \tl_const:Nn \c_@@_notecolor_tl { NankaiPurple } }
        \tcolorbox [
            breakable,
            enhanced~jigsaw,
            sharp~corners,
            colback    = \tl_use:N \c_@@_notecolor_tl !20,
            colframe   = \tl_use:N \c_@@_notecolor_tl,
            toprule    = 0pt,
            rightrule  = 0pt,
            bottomrule = 0pt,
            leftrule   = 3pt,
        ]
        \ignorespaces
    }
    { \endtcolorbox }
%    \end{macrocode}
% \end{environment}
